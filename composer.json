{
    "name": "cypresslab/gitelephant",
    "description": "An abstraction layer for git written in PHP 5.3",
    "keywords": ["git"],
    "homepage": "http://matteosister.github.com/GitElephant",
    "version": "1.0.0-alpha4",
    "license": "LGPLv3",
    "authors": [
        {
            "name": "Matteo Giachino",
            "email": "matteog@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/process": ">=2.1,<2.4-dev",
        "symfony/filesystem": ">=2.1,<2.4-dev",
        "symfony/finder": ">=2.1,<2.4-dev",
        "phpcollection/phpcollection": "~0.3"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "sami/sami": "dev-master"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-0": {
            "GitElephant": "src/"
        }
    }
}
