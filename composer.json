    "description": "An abstraction layer for git written in PHP 5.3",
    "keywords": ["git"],
    "homepage": "http://gitelephant.cypresslab.net/",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Matteo Giachino",
            "email": "matteog@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "symfony/process": "~2.0|~3.0|~4.0",
        "symfony/filesystem": "~2.0|~3.0|~4.0",
        "symfony/finder": "~2.0|~3.0|~4.0",
        "phpcollection/phpcollection": "~0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "~0.9",
