{
    "name": "cypresslab/gitelephant",
    "description": "An abstraction layer for git written in PHP 5.3",
    "keywords": ["git"],
    "homepage": "http://matteosister.github.com/GitElephant",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Matteo Giachino",
            "email": "matteog@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/process": "~2.0",
        "symfony/filesystem": "~2.0",
        "symfony/finder": "~2.0",
        "phpcollection/phpcollection": ">=0.1,<0.3-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "0.8.*",
        "sami/sami": "dev-master"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-0": {
            "GitElephant": "src/"
        }
    }
}
