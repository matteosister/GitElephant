{
    "name": "cypresslab/gitelephant",
    "description": "An abstraction layer for git written in PHP",
    "keywords": [
        "git"
    ],
    "homepage": "http://gitelephant.cypresslab.net/",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Matteo Giachino",
            "email": "matteog@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "phpcollection/phpcollection": "~0.4",
        "symfony/filesystem": "~2.0|~3.0|~4.0",
        "symfony/finder": "~2.0|~3.0|~4.0",
        "symfony/process": "~2.0|~3.0|~4.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.1",
        "phpunit/phpunit": "~7.0",
        "symfony/var-dumper": "3.*|4.*"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-0": {
            "GitElephant": "src/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
